<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acer.controlandoandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/logoroxa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoroxa"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaInicial">
            <intent-filter>
                <action android:name=   "android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TelaCadastro" />
        <activity android:name=".AlteracaoReceita" />
        <activity android:name=".AlterarSenha" />
        <activity android:name=".CadastroReceita" />
        <activity android:name=".AlterarNome" />
        <activity android:name=".CadastroMetas" />
        <activity android:name=".CadastroDespesa" />
        <activity android:name=".TelaPrincipalActivity" />
        <activity android:name=".AlteracaoMeta" />
        <activity android:name=".AlteracaoDespesa"></activity>
    </application>

</manifest>